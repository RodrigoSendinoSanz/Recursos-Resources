import javax.swing.ImageIcon;
import javax.swing.JFrame;

public class Main {

	public static void main(String[] args) {
		// JFrame = es una ventana GUI Interfaz gráfica de usuario donde añadir compnentes
		
		JFrame frame = new JFrame();// crea una ventana
		frame.setTitle("El titulo va aqui");//Pone un titulo a la ventana
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);//cierra la aplicacion
		frame.setResizable(false);//asi no se puede cambair sus medidas
		frame.setSize(420,420);//define el alto y el ancho de la ventana
		frame.setVisible(true);//hace visible a la ventana
			
		ImageIcon image = new ImageIcon("logo.png");		
		
	}

}
